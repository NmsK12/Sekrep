{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "healthcheckPath": "/",
    "restartPolicyType": "ON_FAILURE",
    "sleep": false,
    "startCommand": "npm start",
    "numReplicas": 1,
    "region": "us-east4"
  },
  "service": {
    "name": "sekrep-api",
    "env": "production",
    "ports": [
      3000
    ]
  }
}

